{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "richtext"
    },
    "Price": {
      "type": "biginteger",
      "required": true
    },
    "Stock": {
      "type": "biginteger"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "products"
    },
    "instagram_hashtags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::instagram-hashtag.instagram-hashtag",
      "inversedBy": "products"
    },
    "cost": {
      "type": "biginteger",
      "required": true
    },
    "discount": {
      "type": "integer",
      "max": 100,
      "min": 0
    },
    "Details": {
      "type": "component",
      "repeatable": true,
      "component": "product.details"
    },
    "Sales": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cart"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    }
  }
}
